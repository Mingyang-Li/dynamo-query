{
  "name": "dynamo-query",
  "version": "1.0.0-beta",
  "keywords": [
    "dynamodb",
    "aws",
    "prisma",
    "orm",
    "sdk",
    "nosql",
    "client",
    "typescript",
    "javascript",
    "type-safe"
  ],
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlepine/dynamo-query.git"
  },
  "homepage": "https://github.com/spencerlepine/dynamo-query#readme",
  "scripts": {
    "TODO": "TODO",
    "test:unit": "vitest --config vitest.config.unit.ts",
    "test:e2e": "vitest --config vitest.config.e2e.ts",
    "build": "tsup --dts",
    "prepare": "husky"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@azure/cosmos": "4.2.0",
    "neverthrow": "8.1.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "22.10.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.8.8",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx|json|css|md)": [
      "prettier --write"
    ]
  }
}
